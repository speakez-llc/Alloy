{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run F# Expecto Tests",
            "type": "coreclr",
            "request": "launch",
            "program": "dotnet",
            "args": [
                "run",
                "--project",
                "${workspaceFolder}/tests/Alloy.Tests.fsproj"
            ],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false
        }
    ]
}